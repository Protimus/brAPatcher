QWidget
{
    color: white;
    background: qlineargradient( x1:0 y1:0, x2:1 y2:0, stop:0 #1e4f11, stop:1 #153d0b);
}

QFrame
{
    background-color: white;
    border-radius: 2px;
    border: 1px solid #76797C;
}

QProgressBar {
	background-color: white;
    border-radius: 2px;
    border: 1px solid #76797C;
}

QProgressBar::chunk {
	background-color: #0c3905;
	width: 10px;
	margin: 0.5px;
}
